; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="THWBH"
run/main_scene="res://Start.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

SignalManager="*res://src/autoload/SignalManager.gd"
SteamManager="*res://src/autoload/SteamManager.gd"
Globals="*res://src/autoload/Globals.gd"
Build="*res://src/autoload/Build.gd"
Online="*res://src/autoload/Online.gd"
Nakama="*res://addons/com.heroiclabs.nakama/Nakama.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=640

[internationalization]

locale/translations=PackedStringArray("res://i18n/main.de.translation", "res://i18n/main.en.translation", "res://i18n/main.es.translation", "res://i18n/main.ja.translation", "res://i18n/main.pl.translation", "res://i18n/main.ru.translation", "res://i18n/main.tr.translation", "res://i18n/main.uk.translation", "res://i18n/main.zh_CN.translation", "res://i18n/main.zh_TW.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
